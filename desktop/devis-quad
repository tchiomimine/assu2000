  <script>
    function desactiver() {
      if (document.getElementById("edit-auto-marque")) {
        s = document.getElementById("edit-auto-marque");
      }
      if (document.getElementById("edit-vsp-marque")) {
        s = document.getElementById("edit-vsp-marque");
      }
      if (document.getElementById("edit-moto-marque")) {
        s = document.getElementById("edit-moto-marque");
      }
      if (document.getElementById("edit-cyclo-marque")) {
        s = document.getElementById("edit-cyclo-marque");
      }
      for (i = 0; i < s.options.length; i++) {
        if (s.options[i].value == "0") s.options[i].disabled = true;
      }
    }
