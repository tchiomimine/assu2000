	function displayPassword(object){
           var obj=document.getElementsByName('password')[0];
           if(obj=='undefined')
           		obj = document.getElementById('password');
        if (object.checked) {
            //document.getElementsByName("password")[0].type='text';
            var newO=document.createElement('input');
            newO.setAttribute('type','text');
            newO.setAttribute('id','password');
            newO.setAttribute('name',obj.name);
            newO.setAttribute('value',obj.value);
            newO.setAttribute('className','inputbox');
            newO.setAttribute('class','inputbox');
            obj.parentNode.replaceChild(newO,obj);
        }
