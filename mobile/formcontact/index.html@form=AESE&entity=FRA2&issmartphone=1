<script type="text/javascript">
function submitData()
{
	$('.form-page').hide();
	var post_url = 'confirmation.php'; //get form action url
	var request_method = 'POST'; //get form GET/POST method

   $.ajax({
      type: request_method,
      url: post_url,
      data: $('#form-contact').serialize(),
      datatype : 'html',
      success: function (data) {
    	  $('#resultat').html(data);
      },
   	  error: function(data) {
 		$('#resultat').html("<p class=\"error\">Une erreur s'est produite lors de la transmission de votre demande.</p>");
 	  }
   	});

 	return false;
}


</script>



