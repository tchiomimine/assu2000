  <script>
    $(document).ready(function() {
      $('input.assu-date').on('keyup keydown', function(e){
        if(e.which !== 8) {
            var numChars = jQuery(this).val().length;
            if(numChars === 2 || numChars === 5){
                var thisVal = jQuery(this).val();
                thisVal += '/';
                jQuery(this).val(thisVal);
            }
        }
    });
  });

  </script>

